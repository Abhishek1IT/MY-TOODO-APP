ponents/TodoApp.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"todo-app\">\n    <form @submit.prevent=\"addTodo\">\n      <input \n        type=\"text\" \n        v-model=\"newTodo\" \n        placeholder=\"Add todo...\" \n      />\n      <button type=\"submit\" :disabled=\"!newTodo.trim()\">Add</button>\n    </form>\n\n    <ul>\n      <li v-for=\"todo in todos\" :key=\"todo._id\">\n        <input \n          type=\"checkbox\" \n          v-model=\"todo.completed\" \n          @change=\"updateTodo(todo)\"\n        />\n        <span :class=\"{ done: todo.completed }\">{{ todo.text }}</span>\n        <button @click=\"deleteTodo(todo._id)\" v-if=\"!todo.completed\">Delete</button>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\naxios.defaults.baseURL = \"http://localhost:3000\"; \n\nexport default {\n  name: \"TodoApp\",\n  data() {\n    return {\n      todos: [],\n      newTodo: \"\"\n    };\n  },\n  methods: {\n    async fetchTodos() {\n      try {\n        const res = await axios.get(\"http://localhost:3000/todos\");\n        this.todos = res.data;\n      } catch (err) {\n        console.error(\"Error fetching todos:\", err);\n      }\n    },\n    async addTodo() {\n      if (!this.newTodo.trim()) return;\n      try {\n        const res = await axios.post(\"/todos\", {\n          text: this.newTodo,\n          completed: false\n        });\n        this.todos.push(res.data);\n        this.newTodo = \"\";\n      } catch (err) {\n        console.error(\"Error adding todo:\", err);\n      }\n    },\n    async updateTodo(todo) {\n      try {\n        await axios.put(`/todos/${todo._id}`, todo);\n      } catch (err) {\n        console.error(\"Error updating todo:\", err);\n      }\n    },\n    async deleteTodo(id) {\n      try {\n        await axios.delete(`/todos/${id}`);\n        this.todos = this.todos.filter(todo => todo._id !== id);\n      } catch (err) {\n        console.error(\"Error deleting todo:\", err);\n      }\n    }\n  },\n  mounted() {\n    this.fetchTodos();\n  }\n};\n</script>\n\n<style>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nbody {\n  background-color: #68eddbff;\n  padding: 20px;\n}\n\n.todo-app {\n  padding: 10px;\n  max-width: 500px;\n  margin: auto;\n  font-family: Arial;\n}\n\nform {\n  margin-bottom: 20px;\n}\n\ninput[type=\"text\"] {\n  padding: 8px;\n  width: 70%;\n}\n\nbutton {\n  padding: 8px 12px;\n  margin-left: 10px;\n}\n\nbutton:disabled {\n  opacity: 0.5;\n  cursor: not-allowed;\n}\n\nul {\n  list-style: none;\n  padding: 0;\n}\n\nli {\n  margin-bottom: 10px;\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n}\n\n.done {\n  text-decoration: line-through;\n  color: gray;\n}\n</style>\n"]}]}